{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/.*", "dest": "/api/index.py" },
    { "src": "/sw.js", "dest": "/static/sw.js" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/static/index.html" }
  ]
}
